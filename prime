#include <stdio.h>
int main()
{
    int a,i,count;
    printf("enter any number:");
    scanf("%d",&a);
    if(a==2)
    printf("it is prime");
    else{
    for(i=2;i<a;i++);
    {
        if(a%i==0)
        {
            count++;
        
        }
    }
        if(count==0)
        printf("its a prime");
        else
        printf("its not a prime");
  }  
}
