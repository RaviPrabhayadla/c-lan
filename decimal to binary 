#include<stdio.h>
#include<conio.h>
void main()
{
    int c[32],i=0,q=0,a;
    printf("enter any decimal");
    scanf("%d",&a);
    while(a>0)
    {
        c[i++]=a%2;
        a=a/2;
    }
        for (int j = i - 1; j >= 0; j--)
        {
        printf("%d", c[j]);
        }
    printf("\n");
    }